function count(lo: Int, hi: Int, a: Dummy):Int
  ensures hi <= lo ==> result == 0
  ensures lo <= hi ==> count(lo, hi+1, a) == result + (loc(a, hi) ? 0 : 1)

function recfun(x: Int): Int
  requires x > 0
  ensures recfun(x) < 0
{ -x }

domain Dummy {
  function loc(a: Dummy, i: Int): Bool
}