field f1: Ref

field f2: Ref

method test1(a: Ref)
    requires acc(a.f2, write)
    ensures [true, acc(a.f2, write - perm(a.f1))]
    ensures [true, (forperm [f2] r :: false)]
{
}

method test2(a: Ref)
{
    inhale acc(a.f2, write)
    exhale acc(a.f2, write - perm(a.f1))
    exhale (forperm [f2] r :: false)
}
