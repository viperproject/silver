field f: Int
field b: Bool

predicate P(this: Ref) {
     acc(this.b, write)
  && (this.b ==> acc(this.f, write))
}

method branch(this: Ref)
  requires acc(P(this), write)
{
  assert
       (3 > 2)
    && (unfolding acc(P(this), write)
          in (!this.b ? true : true))    // HERE
}
