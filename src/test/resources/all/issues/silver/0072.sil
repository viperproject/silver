field f: Int

predicate token(x: Ref) {
    acc(x.f)
}

method t_plus(x: Ref)
    requires acc(x.f)
{
    fold acc(token(x), (1/10))
}

method t_minus_1(x: Ref)
    requires acc(x.f)
{
    //:: ExpectedOutput(fold.failed:negative.permission)
    fold acc(token(x), (-1/1))
}

method t_minus_2(x: Ref)
    requires acc(x.f)
{
    //:: ExpectedOutput(fold.failed:negative.permission)
    fold acc(token(x), -(1/1))
}

