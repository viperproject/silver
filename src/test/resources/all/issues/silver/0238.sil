// Test case 1
domain A1 {
  axiom x1 {
    //:: ExpectedOutput(typechecker.error)
    forall s : A1 :: q1(s) ==> p1(s)
  }
}
function p1(s:A1) : Bool
function q1(s:A1) : Bool
  requires p1(s)

// Test case 2
domain A2 {
  axiom x2 {
    //:: ExpectedOutput(typechecker.error)
    forall s : Int :: q2(s) ==> p2(s)
  }
}
function p2(s:Int) : Bool
function q2(s:Int) : Bool
  requires p2(s)

