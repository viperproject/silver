function trigger(i: Int): Bool
{
  true
}

method triggerUse(s: Seq[Int])
  requires |s| > 0
  requires forall i: Int :: {trigger(i)} 0 <= i < |s| ==> s[i] > 0
{
  assert asserting (trigger(0)) in s[0] > 0
}

