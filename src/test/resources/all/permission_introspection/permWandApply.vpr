//:: IgnoreFile(/Carbon/issue/243/)
field f: Int
field g: Int

method m(x:Ref, y: Ref) {
    inhale acc(x.f)
    inhale acc(x.f) --* acc(x.g)

    assert perm(acc(x.f) --* acc(x.g)) == write
    apply acc(x.f) --* acc(x.g)
    assert perm (acc(x.f) --* acc(x.g)) == none
}
