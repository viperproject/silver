field n: Ref

method get_refs() returns (x: Ref, y: Ref)
    ensures acc(x.n)

method foo() {
  var a: Ref, b: Ref

  var c: Ref, d: Ref := get_refs()
  var e: Ref := new(n)

  c.n, e := get_refs()
  c.n.n := new(n)
}
