      && (forall x4: Ref, y4: Ref, z4: Ref :: {acc(x4.f, q) && acc(y4.f, q) && acc(z4.f, q) --* acc(p(x4, y4, z4), q)} x4 in xs4 && y4 in ys4 && z4 in zs4 ==> acc(x4.f, q) && acc(y4.f, q) && acc(z4.f, q) --* acc(p(x4, y4, z4), q))
