field f: Int

method m(x: Ref, y: Ref) {
    inhale acc(x.f) && acc(y.f)

    var as: Seq[Ref]
    as := Seq(x, y)

    assert forall a: Ref :: a in as ==> acc(a.f)

    inhale forall a: Ref :: {a.f} a in as ==> a.f > 0

    assert x.f > 0
}
