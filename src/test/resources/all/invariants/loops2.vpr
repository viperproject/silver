//:: IgnoreFile(/carbon/issue/182/)

method main()
{
     var i:Int := 1
     var j:Bool := true

     assert i <= 5
     if(!(i < 5)) {
         goto skiploop1
     }

     label loop1
      invariant i < 5
         assert i <= 5
         if(!j) {
             goto skiploop2
         }

         label loop2
             j := false
             assert i <= 5
             if(i < 5) {
                 goto loop2
             }
         label skiploop2
         i := i + 1
         assert i <= 5
         if(i < 5) {
             goto loop1
         }
     label skiploop1

     assert(i == 5)
}