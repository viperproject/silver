method main()
{
    var i: Int := 1
    var j: Int := 2
    var k: Int := 3
    var l: Int := 4
    var m: Int := 5
    var n: Int := 7
    var z: Int

    while(m == n) {
        z := (i + j * k) \ n + 2
    }
    assert(l + k + j + n == 100 - n - 20 * k - 7 * j - (n - 1) \ 2 )
}

