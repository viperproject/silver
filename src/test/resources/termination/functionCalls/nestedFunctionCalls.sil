import "../decreaseOrder/int_decreases.sil"

function f(x: Int): Int
decreases x
{
  x > 7 ? f(f(x-x)) : 5
}

function g(x: Int): Int
ensures result == 1
decreases x
{
  x > 1 ? h(g(x-1)-1) : 1
}

