field f:Int

predicate inv(s:Set[Ref]) {
    (forall x : Ref :: x in s ==> acc(x.f, write))
}

method m1(this:Set[Ref])
requires acc(inv(this), write)
ensures acc(inv(this), write)
{
    unfold acc(inv(this), write)
    var t:Ref
    assume (t in this)
    var a:Int := t.f
    fold acc(inv(this), write)
}