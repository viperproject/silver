field f: Int

function fun01(xs: Set[Ref], y: Ref): Int
  requires forall x: Ref :: x in xs ==> acc(x.f)
  requires y in xs
{ y.f }

method test01(xs: Set[Ref], y: Ref) {
  inhale forall x: Ref :: x in xs ==> acc(x.f)
  inhale y in xs
  inhale fun01(xs, y) == 0

  assert y.f == 0
}
