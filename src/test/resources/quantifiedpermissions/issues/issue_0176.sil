field val: Int

method main() {
  var v: Seq[Ref] := Seq[Ref]()
  var i: Int
  var res: Int := 0
  var tmp: Ref

  i := 0

  while (i < 10)
    invariant i <= 10
    invariant |v| == i
    invariant forall j:Int :: j in [0..i) ==> acc(v[j].val)
    invariant forall j:Int :: j in [0..i) ==> v[j].val == 1
  {
    tmp := new(val)
    tmp.val := 1
    v := v ++ Seq(tmp)
    i := i + 1
  }

  assert forall j:Int :: j in [0..10) ==> v[j].val > 0
}
