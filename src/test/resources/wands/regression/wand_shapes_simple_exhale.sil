field f: Ref
field g: Int


method t01(p: Ref, r:Ref) {
  package acc(p.f) --* true
  exhale  acc(p.f) --* true
  
  //:: ExpectedOutput(exhale.failed:wand.not.found)
  exhale  acc(r.f) --* true 
}
  

method t02(x: Ref) {
  inhale acc(P(x))
  unfold P(x) 
  exhale (acc(x.f) --* acc(x.f))
  
  //:: ExpectedOutput(exhale.failed:wand.not.found)
  exhale (acc(x.f) --* acc(x.f))
}

predicate P(x:Ref) {
  acc(x.f) --* acc(x.f)
}

