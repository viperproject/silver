predicate P() {true}
predicate Q() {true}

method test01() {
  package true --* acc(P(), 1/1) {
      fold P()
      fold P()
  }
}

method test02() {
  package true --* acc(P(), 1/1) && acc(P(), 1/1) {
      fold P()
      fold P()
  }
}

method test03() {
  package true --* acc(P(), 2/1) {
    fold P()
    fold P()
  }
}

method test04() {
  package true --* acc(P(), 1/1) && acc(Q(), 1/1) {
      fold P()
      fold Q()
  }
}
