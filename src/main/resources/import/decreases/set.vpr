import "declaration.vpr"

domain SetTerminationOrder[S]{
    //Sets
    axiom set_ax_dec{
        forall set1: Set[S], set2: Set[S] :: {decreasing(set1, set2)}
            (|set1| < |set2|) ==> decreasing(set1, set2)
    }
    axiom set_ax_bound{
        forall set1: Set[S] ::  {bounded(set1)}
            bounded(set1)
    }
}